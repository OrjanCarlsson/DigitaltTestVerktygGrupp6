<Page x:Class="DigitaltTestVerktygGrupp6Student.View.Results"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DigitaltTestVerktygGrupp6Student.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Results">

    <Grid>
        <TextBlock>
            <TextBlock.Text>
                <MultiBinding StringFormat="{}Tack för att du har medverkat. Du fick {0} poäng av {1} möjliga.">
                    <Binding Path="Score" />
                    <Binding Path="TotalScore" />
                </MultiBinding>
            </TextBlock.Text>
        </TextBlock>
    </Grid>
</Page>
